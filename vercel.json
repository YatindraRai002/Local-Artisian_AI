{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "backend/final_server.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "github": {
    "silent": true
  }
}